@{
}
@model int
<div class="form-group">
    <label>Номер лицевого счет</label>
    <input class="form-control" type="number" id="edit-number" />
</div>
<div class="form-group">
    <label>Дата начала действия</label>
    <input class="form-control" type="date" id="edit-dateStart" value="" />
</div>
<div class="form-group">
    <label>Дата окончания действия</label>
    <input class="form-control" type="date" id="edit-dateEnd" value="" />
</div>
<div class="form-group">
    <label>Адрес</label>
    <input class="form-control" type="text" id="edit-address" />
</div>
<div class="form-group">
    <label>Площадь помещения</label>
    <input class="form-control" type="text" id="edit-square" />
</div>
<button onclick="editBankBook(@Model)">Редактировать</button>

<script type="text/javascript">
    window.addEventListener('load', () => {
        loadEdit(@Model);
    });
</script>